{
  "rules": {
    "semi": 0,
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "camelcase": 0,
    "no-undef": 0,
    "no-param-reassign": ["warn", {"props": false}],
    "react/jsx-no-bind": 0
  },
  "extends": "airbnb",
  "plugins": [
    "react",
    "import"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src/"]
      }
    }
  }
}